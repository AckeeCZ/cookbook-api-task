{
  "name": "NodeTemplateAckee",
  "description": "NodeTemplateAckee",
  "version": "3.0.0",
  "author": {
    "email": "pan.unicorn@ackee.cz",
    "name": "Pan Unicorn"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "10.4.1"
  },
  "scripts": {
    "start": "NODE_PATH=./config:./app node server.js",
    "start-win": "set NODE_PATH=./config;./app & node server.js",
    "ci-test": "NODE_PATH=./config:./app node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha --use_strict test/setup.js test/** --  -R tap > test.tap; istanbul report clover; exit 0",
    "ci-lint": "eslint -f checkstyle . > checkstyle-result.xml; exit 0",
    "test": "NODE_PATH=./config:./app node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha --use_strict test/setup.js test/**",
    "test-win": "set NODE_PATH=./config;./app & node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha --use_strict test/setup.js test/**",
    "lint": "eslint .",
    "jsdoc": "jsdoc -r -c ./conf.json ./",
    "cover": "NODE_PATH=./config:./app node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha --use_strict test/setup.js test/**",
    "cover-win": "set NODE_PATH=./config;./app; & node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha --use_strict test/setup.js test/integration/* test/unit/* & exit 0",
    "postinstall": "node_modules/.bin/bower install"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "bower": "1.8.4",
    "compression": "1.7.2",
    "connect-flash": "0.1.1",
    "connect-mongo": "2.0.1",
    "cookie-parser": "1.3.5",
    "cookie-session": "1.3.2",
    "cors": "2.7.1",
    "csurf": "1.8.3",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "express-winston": "1.3.1",
    "i18n-2": "0.7.2",
    "lodash": "4.17.10",
    "method-override": "2.3.10",
    "mongoose": "5.1.5",
    "morgan": "1.9.0",
    "swig": "1.4.2",
    "use-strict": "1.0.1",
    "view-helpers": "0.1.5",
    "winston": "2.4.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "expect": "1.20.2",
    "expect.js": "0.3.1",
    "istanbul": "0.4.5",
    "knex-cleaner": "1.1.4",
    "mocha": "3.5.3",
    "nodemon": "1.17.5",
    "should": "13.2.1",
    "supertest": "3.1.0"
  }
}
